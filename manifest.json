{
  "manifest_version": 2,

  "name": "Swinger",
  "version": "0.1.0",
  "description": "Your Swinger assistant.",

   "content_scripts": [
      {
          "matches": [
              "http://*/*",
              "https://*/*"
          ],
          "css": [
              "styles.css"
          ],
          "js": [
              "external/jquery.min.js",
              "content.js"
          ],
          "run_at" : "document_end"
      }
  ]

}
